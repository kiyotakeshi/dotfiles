        {
            "text": "failed",
            "color": "#f53838",
            "border": "1px solid white",
            "backgroundColor": "#091d0f"
        },
        {
            "text": "error",
            "color": "#f53838",
            "border": "1px solid white",
            "backgroundColor": "#091d0f"
        },
        {
            "text": "exception",
            "color": "#f53838",
            "border": "1px solid white",
            "backgroundColor": "#091d0f"
        },
        {
            "text": "TODO:",
            "color": "#091d0f",
            "border": "1px solid red",
            "backgroundColor": "#f5f3f3"
        }
